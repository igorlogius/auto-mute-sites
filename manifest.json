{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/mute-sites",
  "browser_action": {
    "default_area": "navbar"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "474": "icon.png"
  },
  "manifest_version": 2,
  "name": "Mute Sites",
  "description": "Mute or Unmute entire websites instead of individual tabs",
  "permissions": ["storage", "tabs"],
  "version": "1.1.5"
}
